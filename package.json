{
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "govuk-frontend": "^5.3.1"
  },
  "scripts": {
    "start": "parcel --dist-dir gchqnet/static_compiled/",
    "build": "parcel build"
  },
  "targets": {
    "css": {
      "source": "assets/css/main.scss",
      "distDir": "gchqnet/static_compiled/css"
    },
    "js": {
      "source": "assets/js/main.js",
      "outputFormat": "global",
      "distDir": "gchqnet/static_compiled/js"
    }
  }
}
