{
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "parcel": "^2.12.0"
  },
  "dependencies": {
    "@russss/maplibregl-layer-switcher": "^1.2.4",
    "govuk-frontend": "^5.3.1",
    "maplibre-gl": "^4.2.0"
  },
  "scripts": {
    "start": "parcel --dist-dir gchqnet/static_compiled/",
    "build": "parcel build"
  },
  "targets": {
    "css": {
      "source": "assets/main.scss",
      "distDir": "gchqnet/static_compiled/"
    },
    "js": {
      "source": "assets/main.js",
      "outputFormat": "global",
      "distDir": "gchqnet/static_compiled/"
    },
    "mapjs": {
      "source": "assets/map/map.ts",
      "outputFormat": "global",
      "distDir": "gchqnet/static_compiled/"
    }
  },
  "@parcel/resolver-default": {
    "packageExports": true
  }
}
